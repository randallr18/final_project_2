<h1>Record a Workout!</h1>
<hr>
<br><br>
<%= form_with model: @userexercise do |f|  %>
<!-- <%= f.collection_select :user_id, User.all, :id, :name %> -->
<%= f.label :exercise_id, 'Choose an Exercise!' %>
